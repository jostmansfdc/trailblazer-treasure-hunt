{
  "name": "trailblazer-treasure-hunt",
  "description": "Trailblazer Treasure Hunt app showing how to build scalable apps that integrate with Salesforce Lightning Platform",
  "repository": "https://github.com/lekkimworld/trailblazer-treasure-hunt",
  "addons": [
    "cloudamqp",
    "herokuconnect",
    "papertrail",
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "CLOUDAMQP_APIKEY": {
      "required": true
    },
    "CLOUDAMQP_URL": {
      "required": true
    },
    "DATABASE_URL": {
      "required": true
    },
    "NODE_ENV": {
      "required": false
    },
    "PAPERTRAIL_API_TOKEN": {
      "required": true
    },
    "PERSONACCOUNT_RECORDTYPEID": {
      "required": true
    },
    "REDIS_URL": {
      "required": true
    },
    "SESSION_SECRET": {
      "required": false
    },
    "SF_CALLBACK_URL": {
      "required": true
    },
    "SF_CLIENT_ID": {
      "required": true
    },
    "SF_CLIENT_SECRET": {
      "required": true
    },
    "SF_PASSWORD": {
      "required": true
    },
    "SF_USERNAME": {
      "required": true
    }
  },
  "formation": {
    "backend": {
      "quantity": 1
    },
    "web": {
      "quantity": 1
    }
  },
  "scripts": {
  },
  "stack": "heroku-18"
}
